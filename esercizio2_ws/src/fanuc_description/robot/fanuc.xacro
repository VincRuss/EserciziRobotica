<?xml version="1.0" encoding="utf-8"?>

<robot  name="fanucM20iA" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- ******************** Used for fixing robot to Gazebo 'base_link' ********************-->
    <link name="world"/>

    <joint name="virtual_joint" type="fixed">
        <parent link="world"/>
        <child link="base_link"/>
    </joint>

    <!--************************************************BASE_LINK********************************************-->
    <link name="base_link">
        <!--link properties-->
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_0.dae" />
            </geometry>
            <material name=""/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_0.stl" />
            </geometry>
        </collision>
    </link>

     <!--************************************************LINK 1********************************************-->
    <link name="link1">
        <!--Link properties-->
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_1.dae" />
            </geometry>
            <material name=""/>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_1.stl" />
            </geometry>
        </collision>
    </link>

    <joint name="joint1" type="revolute">
        <!--Joint properties-->
        <origin xyz="0 0 0" rpy="0 0 0" />
        <parent link="base_link" />
        <child link="link1" />
        <axis xyz="0 0 1" />
        <limit lower="-3.23" upper="3.23" effort="12" velocity="3.4" />
    </joint>

     <!--************************************************LINK 2********************************************-->
    <link name="link2">
        <!--Link properties-->
        <visual>
            <origin xyz="-0.15 -0.525 0" rpy="-${pi/2} 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_2.dae" />
            </geometry>
            <material name=""/>
        </visual>
        
        <collision>
            <origin xyz="-0.15 -0.525 0" rpy="-${pi/2} 0 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_2.stl" />
            </geometry>
        </collision>
    </link>

    <joint name="joint2" type="revolute">
        <!--Joint properties-->
        <origin xyz="0.15 0 0.525" rpy="${pi/2} 0 0" />
        <parent link="link1" />
        <child link="link2" />
        <axis xyz="0 0 1" />
        <limit lower="-1.75" upper="2.79" effort="12" velocity="3.05" />
    </joint>

    <!--************************************************LINK 3********************************************-->
    <link name="link3">
        <!--Link properties-->
        <visual>
            <origin xyz="-1.315 0 0.15" rpy="-${pi/2} 0 -${pi/2}" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_3.dae" />
            </geometry>
             <material name=""/>
        </visual>

        <collision>
            <origin xyz="-1.315 0 0.15" rpy="-${pi/2} 0 ${pi/2}" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_3.stl" />
            </geometry>
        </collision>

    </link>

    <joint name="joint3" type="revolute">
        <!--Joint properties-->
        <origin xyz="0 0.79 0" rpy="0 0 ${pi/2}" />
        <parent link="link2" />
        <child link="link3" />
        <axis xyz="0 0 1" />
        <limit lower="-3.23" upper="4.81" effort="12" velocity="3.14" />
    </joint>

     <!--************************************************LINK 4********************************************-->
    <link name="link4">
        <!--Link properties-->
        <visual>
            <origin xyz="-1.465 0 -0.15" rpy="-${pi} -${pi/2} 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_4.dae" />
            </geometry>
            <material name=""/>
        </visual>

        <collision>
            <origin xyz="-1.465 0 -0.15" rpy="-${pi} -${pi/2} 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_4.stl" />
            </geometry>
        </collision>
    </link>

    <joint name="joint4" type="revolute">
        <!--Joint properties-->
        <origin xyz="0.15 0 0" rpy="${pi/2} 0 0" />
        <parent link="link3" />
        <child link="link4" />
        <axis xyz="0 0 1" />
        <limit lower="-3.49" upper="3.49" effort="11.7" velocity="7.07" />
    </joint>

    <!--************************************************LINK 5********************************************-->
    <link name="link5">
        <!--Link properties-->
        <visual>
            <origin xyz="-1.465 1.01 0" rpy="-${pi/2} 0 -${pi/2}" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_5.dae" />
            </geometry>
            <material name=""/>
        </visual>

        <collision>
            <origin xyz="-1.465 1.01 0" rpy="-${pi/2} 0 -${pi/2}" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_5.stl" />
            </geometry>
        </collision>
    </link>
    
    <joint name="joint5" type="revolute">
        <!--Joint properties-->
        <origin xyz="0 0 0.86" rpy="-${pi/2} 0 0" />
        <parent link="link4" />
        <child link="link5" />
        <axis xyz="0 0 1" />
        <limit lower="-2.44" upper="2.44" effort="11.7" velocity="7.07" />
    </joint>

    <!--************************************************LINK 6********************************************-->
    <link name="link6">
        <!--Link properties-->
        <visual>
            <origin xyz="-1.465 1.01 0" rpy="-${pi} -${pi/2} 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/visual/link_6.dae" />
            </geometry>
            <material name=""/>
        </visual>

        <collision>
            <origin xyz="-1.465 1.01 0" rpy="-${pi} -${pi/2} 0" />
            <geometry>
                <mesh filename="package://fanuc_description/meshes/collision/link_6.stl" />
            </geometry>
        </collision>
    </link>

    <joint name="joint6" type="revolute">
        <!--Joint properties-->
        <origin xyz="0 0 0" rpy="${pi/2} 0 0" />
        <parent link="link5" />
        <child link="link6" />
        <axis xyz="0 0 1" />
        <limit lower="-7.85" upper="7.85" effort="5.8" velocity="10.73" />
    </joint>

    <!--************************************************FLANGE********************************************-->
    <link name="flange"/>

    <joint name="flange" type="fixed">
        <!--Joint properties-->
        <origin xyz="0 0 0.1" rpy="0 0 0" />
        <parent link="link6" />
        <child link="flange" />
    </joint>

</robot>